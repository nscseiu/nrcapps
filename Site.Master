        $("#ctl00_ContentPlaceHolder1_TextExpiredDays").click(function () {
            var StartExpiryDate = $("#ctl00_ContentPlaceHolder1_TextStartExpiryDate").val().split(' - ');
            var StartDate       = StartExpiryDate[0].split(' ');
            var ExpiryDate      = StartExpiryDate[1].split(' '); 
            var date1           = StartDate[0].trim();
            var date2           = ExpiryDate[0].trim();
           
            var oneDay          = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds
            var firstDate       = new Date(2008, 01, 12);
            var secondDate      = new Date(2008, 01, 22);

            var diffDays = Math.round(Math.abs((parseDate(date1).getTime() - parseDate(date2).getTime()) / (oneDay)));

            $('#ctl00_ContentPlaceHolder1_TextExpiredDays').val(diffDays);

            function parseDate(str) {
                var mdy = str.split('/');
                return new Date(mdy[2], mdy[1]-1, mdy[0]);
            }
             
        });


